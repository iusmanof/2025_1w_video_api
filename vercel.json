{
  "version": 2,
  "name": "2025_1w_video_api",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "rewrites": [
    {"source": "/(.*)", "destination": "/"}
  ]
}
